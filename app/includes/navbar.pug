      #signupSection.container.site-content.d-none
        .row.justify-content-center.align-items-center
          .col-md-7.col-lg-5
            h2.text-center Sign Up
            form#signUpForm(method='post' action='#/register' novalidate='')
              input(type='hidden' name='csrfmiddlewaretoken' value='ftjvK2XgajNEbFIJuM2BOaRYPdO7qK4mphmJiqbZN78d2pfkOGtdYjqWVJ2bDSn1')
              .form-group
                input#id_signup_email.form-control(type='email' name='signup_email' placeholder='E-mail address*' required='')
              .form-group
                input#id_signup_password1.form-control(type='password' name='signup_password1' placeholder='Password*' required='')
              .form-group
                input#id_signup_password2.form-control(type='password' name='signup_password2' placeholder='Password confirmation*' required='')
              .form-group
                small *Required Fields
              br
              .row.nopadding
                .form-check.col-12.col-sm-6.nopadding
                  .form-group
                    label.container-checkbox
                      | I ACCEPT CONDITIONS
                      input(type='checkbox' name='signup_term')
                      span.checkmark
                .col-12.col-sm-6.text-center.text-sm-right.nopadding
                  button.btn.btn-primary(type='submit') Create account
            .form-group.text-center
              p
                button.text-uppercase.btn.btn-clear.blue.mt-50(data-switch-to='login')
                  | Back to log in 
                  i.arrow-icon
      #passwordRecoverySection.container.d-none
        .row
          .col-12.col-md-6
            h2.text-center Password Recovery
            form#passRecoveryForm(method='POST' action='#/forgot-password' novalidate='')
              input(type='hidden' name='csrfmiddlewaretoken' value='uBnbJ7EY89yoQaRnZkczJx5uyqtdOTZkEpqphvSHLXTXHUoYjeDbTGEsEWHh11iZ')
              .form-group
                input#id_recovery_email.form-control(type='text' name='recovery_email' maxlength='254' placeholder='E-mail*' required='')
              br
              .form-group.text-center
                button.btn.btn-primary(type='submit') Send
                p
                  button.text-uppercase.btn.btn-clear.blue.mt-50(type='button' data-switch-to='login')
                    | Back to log in
                    i.arrow-icon
          .col-12.col-md-6
            h2.text-center Instructions
            ul.list-custom.list-decimal-style
              li Type in your e-mail address in the field and click SEND.
              li
                | Once you receive the automated e-mail, click the blue 
                strong RECOVER PASSWORD
                |  button to start the recovery procedure.
              li Type in and confirm your new password.
              li Log in using the new credentials.
      #activationEmailSentSection.text-center.d-none
        h2 E-mail was sent
        p
          | E-mail address assigned to this account requires confirmation. Check your e-mail for further instructions.
        p
          button.text-uppercase.btn.btn-clear(type='button' data-switch-to='login') Back to log in
      #recoveryEmailSentSection.text-center.d-none
