        .row.align-self-center.justify-content-center
          .login-form
            h2.text-center Please sign in
            form#loginForm(method='POST' action='#/login' novalidate='' autocomplete='off')
              input(type='hidden' name='csrfmiddlewaretoken' value='uZ1BMrYyUnKmNiY5YWnocp4rqCA7Kb1KEN4PkPchxb5VE2vGiQO0myDpw8ObXjkp')
              .form-group
                input#id_login_username.form-control(type='text' name='login_username' autocomplete='off' maxlength='254' placeholder='E-mail*' required='')
